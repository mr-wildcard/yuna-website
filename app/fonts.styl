@font-face
  font-family 'fontello'
  src url('/fonts/fontello.eot?73636807')
  src url('/fonts/fontello.eot?73636807#iefix') format('embedded-opentype'),
url('/fonts/fontello.woff?73636807') format('woff'),
url('/fonts/fontello.ttf?73636807') format('truetype'),
url('/fonts/fontello.svg?73636807#fontello') format('svg')
font-weight normal
font-style normal

[class^="icon-"]:before, [class*=" icon-"]:before
  font-family "fontello"
  font-style normal
  font-weight normal
  speak none
  display inline-block
  text-decoration inherit
  text-align center
  font-variant normal
  text-transform none
  line-height 1em
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-twitter
  &:before
    content '\e800'

.icon-linkedin
  &:before
    content '\e801'

.icon-pinterest-1
  &:before
    content '\e802'

.icon-paper-plane
  &:before
    content '\e803'
